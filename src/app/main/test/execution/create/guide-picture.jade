md-dialog(aria-label='向导图' ng-cloak)
  md-toolbar(class='md-accent')
    div(class='md-toolbar-tools')
      h2 接线图
      span(flex)
      md-button(class='md-icon-button' ng-click='cancel()')
        md-icon(md-svg-src='assets/icons/ic_close_24px.svg' aria-label='Close Dialog')
  md-dialog-content
    div(class='md-dialog-content')
      h1 测试执行接线图
      img(src='assets/images/guide.jpg')
    div(layout='row')
      span(flex)
      md-button(class='md-raised md-accent' ng-click='startExecution()') {{executeButton}}
